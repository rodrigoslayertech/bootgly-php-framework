parameters:
   excludePaths:
      analyse:
         - */&/*
         - */vs/*
         - */tests/*
         - */examples/*
   reportUnmatchedIgnoredErrors: false
   level: 1

   paths:
      - ../Bootgly
   ignoreErrors:
      -
         identifier: property.uninitializedReadonly
      -
         identifier: property.private
      -
         identifier: property.protected